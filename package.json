{
    "name": "latte-carousel",
    "version": "1.2.1",
    "description": "LatteCarousel is a lightweight and responsive carousel.",
    "repository": "https://github.com/latte-carousel/latte-carousel.git",
    "author": {
        "name": "LatteCarousel",
        "url": "https://latte-carousel.github.io"
    },
    "license": "MIT",
    "private": false,
    "main": "dist/latte-carousel.min.js",
    "types": "dist/lib/index.d.ts",
    "keywords": [
        "carousel",
        "responsive",
        "javascript",
        "slider",
        "typescript"
    ],
    "homepage": "https://latte-carousel.github.io",
    "scripts": {
        "build": "webpack",
        "watch": "webpack --watch",
        "serve": "http-server './'",
        "test": "jest",
        "format": "yarn run format:prettier && yarn run format:csscomb",
        "format:prettier": "prettier --find-config-path --write './src/**' './test/**' './example/**'",
        "format:csscomb": "csscomb './src/scss' './example'",
        "lint": "yarn run lint:tslint && yarn run lint:eslint",
        "lint:tslint": "tslint -c 'tslint.json' './**/*.ts'",
        "lint:eslint": "eslint './**/*.js'"
    },
    "devDependencies": {
        "@types/jest": "^23.3.1",
        "autoprefixer": "^9.1.1",
        "clean-webpack-plugin": "^0.1.19",
        "css-loader": "^1.0.0",
        "csscomb": "^4.2.0",
        "cssnano": "^4.0.5",
        "eslint": "^5.4.0",
        "http-server": "^0.11.1",
        "jest": "^23.5.0",
        "mini-css-extract-plugin": "^0.4.1",
        "node-sass": "^4.9.3",
        "postcss-loader": "^3.0.0",
        "prettier": "1.14.2",
        "sass-loader": "^7.1.0",
        "ts-jest": "^23.1.3",
        "ts-loader": "^4.5.0",
        "tslint": "^5.11.0",
        "typescript": "^3.0.1",
        "webpack": "^4.16.5",
        "webpack-command": "^0.4.1"
    }
}
